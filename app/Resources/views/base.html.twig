<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('font-awesome/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/cover.css') }}">
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    </head>
    <body>
    <section id="menu-area">
        <nav class="navbar main-navbar " role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- LOGO -->
                    <a class="navbar-brand logo" href="{{ path('homepage') }}"><img src="{{ asset('img/Logo Copy.png') }}" class="img-responsive" alt=""></a>
                </div>
                <div id="navbar" class=" pull-right navbar-collapse collapse">
                    <ul id="top-menu" class="nav navbar-nav main-nav menu-scroll">
                        <li><a href=""><i class="fa fa-facebook-official fa-1x" aria-hidden="true"></i></a></li>
                        <li><a href=""><i class="fa fa-twitter fa-1x" aria-hidden="true"></i></a></li>
                        <li><a href="">MENU&nbsp;<i aria-hidden="true" class="fa fa-bars fa-1x"></i></a></li>
                        <li><a href=""><i aria-hidden="true" class="fa fa-arrows-alt fa-1x"></i></a></li>
                    </ul>
                </div><!--/.nav-collapse -->

            </div>
        </nav>
    </section>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
        <script   src="https://code.jquery.com/jquery-3.0.0.min.js"   integrity="sha256-JmvOoLtYsmqlsWxa7mDSLMwa6dZ9rrIdtrrVYRnDRH0="   crossorigin="anonymous"></script>
    <script src="{{ asset('main.js') }}"></script>
    </body>
</html>
